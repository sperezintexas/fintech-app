# VPC Configuration for accounts without default VPC
# This creates resources in the first available VPC/subnet
#
# If you have a specific VPC, uncomment and set these values:
# option_settings:
#   aws:ec2:vpc:
#     VPCId: vpc-xxxxxxxxx
#     Subnets: subnet-xxxxxxxxx
#     AssociatePublicIpAddress: true

# For accounts without default VPC, EB will use these settings
# to find an available VPC automatically
option_settings:
  aws:ec2:vpc:
    AssociatePublicIpAddress: true
  
  aws:autoscaling:launchconfiguration:
    # Security group will be created in the specified VPC
    SecurityGroups: ""
